{
  "name": "wiki-engine",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "react-router-dom": "^4.0.0",
    "react-scripts": "0.9.5",
    "sass-lint": "^1.10.2"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-sass": "npm run build-css && node-sass src/ -o src/ --watch --recursive && wait",
    "start": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "predeploy": "npm run build ",
    "deploy":"cd ./build & surge",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
